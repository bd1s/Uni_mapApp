{
  "expo": {
    "name": "uni_map",
    "slug": "uni_map",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/pinlogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/pinlogo2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ]
    ],
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCSyvolGooAFTh7AGtHUXF92MWHoPTrt_4"
        },
        "icon": {
          "adaptiveIcon": {
            "foregroundImage": "./assets/pinlogo.png",
            "backgroundColor": "#ffffff"
          }
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/pinlogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.fakhreeddine.uni_map"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8662c0fe-7d9f-482f-86e6-5906f44f5adb"
      }
    },
    "androidStatusBar": {
      "backgroundColor": "#ffffff00",
      "translucent": true
    }
  }
}
